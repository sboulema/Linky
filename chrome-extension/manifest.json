{
  "manifest_version": 2,

  "name": "Linky Extension",
  "short_name": "Linky",
  "description": "Add new links to Linky.",
  "version": "1.3",

  "icons": { 
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png" 
  },

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://api.myjson.com/*"
  ]
}
